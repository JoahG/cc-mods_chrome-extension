{
	"manifest_version": 2,

	"name": "Codecademy Moderator Extension",
	"description": "Adds Moderator Tools to Codecademy profile pages.",
	"version": "0.1",

	"icons": { 
		"16": "icon_16.png",
		"48": "icon_48.png",
		"128": "icon_128.png" 
	},

	"permissions": [
		"http://codecademy.com/"
	],

	"content_scripts": [ {
		"all_frames": true,
		"js": [ "jquery.min.js", "magic.js" ],
		"matches": [ "http://www.codecademy.com/*", "https://www.codecademy.com/*" ]
	} ]
}